<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sunzy.vulfocus.mapper.TaskInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.sunzy.vulfocus.model.po.TaskInfo">
        <id column="task_id" property="taskId" />
        <result column="user_id" property="userId" />
        <result column="task_name" property="taskName" />
        <result column="task_status" property="taskStatus" />
        <result column="task_start_date" property="taskStartDate" />
        <result column="task_end_date" property="taskEndDate" />
        <result column="operation_type" property="operationType" />
        <result column="operation_args" property="operationArgs" />
        <result column="task_msg" property="taskMsg" />
        <result column="is_show" property="show" />
        <result column="create_date" property="createDate" />
        <result column="update_date" property="updateDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        task_id, user_id, task_name, task_status, task_start_date, task_end_date, operation_type, operation_args, task_msg, is_show, create_date, update_date
    </sql>

</mapper>
